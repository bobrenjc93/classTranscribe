<html>
  <head>
    <title>Class Transcribe</title>

    <!-- Begin Javsacript -->
    <script src="/javascripts/libraries/jquery-1.11.2.min.js"></script>
    {{#className}}
      <script src="/javascripts/data/videos/{{className}}.js"></script>
    {{/className}}

    <script>
      className = "{{className}}";
    </script>

    <script src="/javascripts/controllers/viewer.js"></script>

    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-29520426-2', 'auto');
